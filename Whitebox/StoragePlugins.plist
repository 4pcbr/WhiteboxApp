<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>SFTPStoragePlugin</key>
	<dict>
		<key>PrivateKey</key>
		<string>~/.ssh/id_rsa</string>
		<key>PublickKey</key>
		<string>~/.ssh/id_rsa.pub</string>
		<key>User</key>
		<string>my_user</string>
		<key>Host</key>
		<string>ftp.example.com</string>
		<key>Port</key>
		<string>22</string>
		<key>WebPort</key>
		<string>80</string>
		<key>WebSchema</key>
		<string>http</string>
		<key>ScriptFilePath</key>
		<string>SFTPStoragePlugin.js</string>
		<key>Enabled</key>
		<false/>
	</dict>
	<key>RESTStoragePlugin</key>
	<dict>
		<key>ScriptFilePath</key>
		<string>RESTStoragePlugin.js</string>
		<key>ParamName</key>
		<string>file</string>
		<key>Path</key>
		<string>/upload</string>
		<key>Host</key>
		<string>example.com</string>
		<key>Scheme</key>
		<string>https</string>
		<key>Enabled</key>
		<false/>
	</dict>
	<key>LocalFileStoragePlugin</key>
	<dict>
		<key>StorePath</key>
		<string>~/Screenshots/</string>
		<key>Enabled</key>
		<true/>
	</dict>
</dict>
</plist>
